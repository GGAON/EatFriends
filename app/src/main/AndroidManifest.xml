<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zeronine.project1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Project1">
        <activity android:name=".intro.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".intro.LoginActivity"/>
        <activity android:name=".intro.SignUpActivity"/>
        <activity android:name=".home.MakeGroupActivity"/>
        <activity android:name=".home.WaitingGroupActivity"/>
        <activity android:name=".home.JoinGroupActivity"/>

    </application>

</manifest>